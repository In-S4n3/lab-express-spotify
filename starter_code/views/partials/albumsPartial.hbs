<div class="card" style="width: 33%;">
  <img src="{{this.images.0.url}}" class="card-img-top" alt="Albuns Images">
  <div class="card-body">
    <h5 class="card-title">{{this.name}}</h5>
    <p class="card-text"></p>
    <button type="submit" class="btn btn-primary" onclick="location.href='/tracks/{{ this.id }}'">View Tracks</button>
  </div>
</div>
<a href="/">Return to Search Bar</a>